<head>
     <link rel="stylesheet" href="css/style-cadastro.css">
</head>

<header>

    <table id="cabecalho">
        <thead>
            <tr>
                <th id="logo">
                    <h1>8RMS</h1>
                </th>
                <th id="inicio">
                    <h4>In√≠cio</h4>
                </th>
                <th id="sobre">
                    <a href="./sobre.html">
                        <h4>Sobre</h4>
                    </a>
                </th>
                <th id="contato">
                    <h4>Contato</h4>
                </th>
            </tr>
        </thead>
    </table>

</header>

<main class="login">
    <div class="container">
        <div class="box">
            <div id="login">
                <img src="./login-icon.png" alt="login icon" id="icon-login">
            </div>
            <form onsubmit="TestarCPF()">
                <div id="itens">
                    Nome: <input type="text" id="name" required>
                    CPF: <input type="text" id="cpf" maxlength="14" onkeydown="javascript: fMasc(this, mCPF)"
                        required><br>
                    E-mail: <input type="email" id="e-mail" required><br>
                    Data de Nascimento: <input type="date" id="date" required><br>
                    Senha: <input type="password" id="password" required>
                    Confirmar senha: <input type="password" id="password-c" required>
                </div>
                <div id="confirmar">
                    <button id="enviar">Cadastrar-se</button>
                </div>
            </form>
        </div>
    </div>
</main>

<footer>
    <p>8RMS</p>
</footer>